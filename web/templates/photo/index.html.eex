<ol class="breadcrumb">
  <li class="breadcrumb-item"><%= gettext "Photos" %></li>
</ol>

<div class="card-group card-group-photos" id="photos">
  <%= for photo <- @photos.entries do %>
    <%= render Helheim.PhotoView, "thumb.html", conn: @conn, photo: photo %>
  <% end %>
</div>

<%= lpaginate @conn, @photos %>
